{
  "$schema": "./node_modules/oxfmt/configuration_schema.json",
  "useTabs": true,
  "tabWidth": 2,
  "endOfLine": "lf",
  "printWidth": 100,
  "bracketSameLine": false,
  "bracketSpacing": true,
  "quoteProps": "preserve",
  "semi": true,
  "singleQuote": false,
  "jsxSingleQuote": false,
  "trailingComma": "es5",
  "arrowParens": "always",
  "htmlWhitespaceSensitivity": "css",
}
